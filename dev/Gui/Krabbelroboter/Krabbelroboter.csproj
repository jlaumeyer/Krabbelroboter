<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\offline.png" />
    <None Remove="images\online.png" />
    <None Remove="images\spitze-links-neon.png" />
    <None Remove="images\spitze-links.png" />
    <None Remove="images\spitze-oben-neon.png" />
    <None Remove="images\spitze-oben.png" />
    <None Remove="images\spitze-rechts-neon.png" />
    <None Remove="images\spitze-rechts.png" />
    <None Remove="images\spitze-unten-neon.png" />
    <None Remove="images\spitze-unten.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Extended.Wpf.Toolkit" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="images\offline.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="images\online.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="images\spitze-links-neon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
